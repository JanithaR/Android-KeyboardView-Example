            android:codes="84"
            android:iconPreview="@drawable/caps_t"
            android:keyIcon="@drawable/caps_t" />
        <Key
            android:codes="89"
            android:iconPreview="@drawable/caps_y"
            android:keyIcon="@drawable/caps_y" />
        <Key
            android:codes="85"
            android:iconPreview="@drawable/caps_u"
            android:keyIcon="@drawable/caps_u" />
        <Key
            android:codes="73"
            android:iconPreview="@drawable/caps_i"
