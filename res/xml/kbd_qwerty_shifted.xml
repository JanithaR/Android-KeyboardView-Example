        <Key
            android:codes="66"
            android:iconPreview="@drawable/caps_b"
            android:keyIcon="@drawable/caps_b" />
        <Key
            android:codes="78"
            android:iconPreview="@drawable/caps_n"
            android:keyIcon="@drawable/caps_n" />
        <Key
            android:codes="77"
            android:iconPreview="@drawable/caps_m"
            android:keyIcon="@drawable/caps_m" />
