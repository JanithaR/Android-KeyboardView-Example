    android:keyWidth="10%p"
    android:verticalGap="@dimen/password_keyboard_verticalGap" >

    <Row android:rowEdgeFlags="top" >
        <Key
            android:codes="81"
            android:iconPreview="@drawable/caps_q"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/caps_q" />
        <Key
            android:codes="87"
            android:iconPreview="@drawable/caps_w"
            android:keyIcon="@drawable/caps_w" />
        <Key
            android:codes="69"
            android:iconPreview="@drawable/caps_e"
            android:keyIcon="@drawable/caps_e" />
        <Key
            android:codes="82"
            android:iconPreview="@drawable/caps_r"
