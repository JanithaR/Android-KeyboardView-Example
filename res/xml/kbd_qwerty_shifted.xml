            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/caps_l" />
    </Row>
    <Row>
        <Key
            android:codes="-1"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyWidth="15%p"
            android:iconPreview="@drawable/shift"
            android:keyIcon="@drawable/shift" />
        <Key
            android:codes="90"
            android:iconPreview="@drawable/caps_z"
            android:keyIcon="@drawable/caps_z" />
        <Key
